{
	"plugins": [
		"prettier-plugin-tailwindcss",
		"prettier-plugin-organize-imports"
	],
	"tabWidth": 2,
	"useTabs": true,
	"endOfLine": "lf",
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"bracketSameLine": true,
	"jsxSingleQuote": true,
	"semi": false,
	"overrides": [
		{
			"files": ["package.json"],
			"options": {
				"useTabs": false
			}
		}
	]
}
